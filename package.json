{
  "name": "rekognition",
  "version": "0.0.0-semantically-released",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/gasserandreas/rekognition.git"
  },
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link": "^1.2.6",
    "apollo-link-context": "^1.0.12",
    "apollo-link-error": "^1.1.5",
    "apollo-link-http": "^1.5.9",
    "apollo-upload-client": "^10.0.0",
    "eslint-plugin-react-hooks": "^1.5.1",
    "formik": "^1.4.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "grommet": "2.6.5",
    "grommet-icons": "4.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^8.0.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "ric-shim": "^1.0.0",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2",
    "yup": "^0.26.6"
  },
  "scripts": {
    "start": "cross-env HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:coverage": "npm test -- --coverage",
    "test:update": "npm test -- --updateSnapshot",
    "lint": "eslint -c ./.eslintrc './src/**/{*.js,*.jsx}'",
    "lint:fix": "eslint -c ./.eslintrc './src/**/{*.js,*.jsx}' --fix",
    "release": "aws s3 sync --delete ./build s3://529821714029-rekognition.gasserandreas.com/",
    "env-push-prod": "aws s3 cp .env.production.local s3://529821714029-rekognition.gasserandreas.com-utils/",
    "env-push-test": "aws s3 cp .env.development.local s3://529821714029-rekognition.gasserandreas.com-utils/",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@storybook/addon-a11y": "5.0.6",
    "@storybook/addon-actions": "5.0.6",
    "@storybook/addon-centered": "5.0.5",
    "@storybook/addon-console": "1.1.0",
    "@storybook/addon-info": "5.0.6",
    "@storybook/addon-knobs": "5.0.6",
    "@storybook/addon-links": "5.0.6",
    "@storybook/addon-viewport": "5.0.6",
    "@storybook/addons": "5.0.6",
    "@storybook/react": "5.0.6",
    "babel-eslint": "9.0.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "jest-fetch-mock": "^2.1.1",
    "jest-styled-components": "^6.3.1",
    "prettier": "^1.17.0",
    "react-app-polyfill": "^0.2.0",
    "redux-mock-store": "^1.5.3",
    "semantic-release": "^15.13.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/{*.js,*.jsx,*.ts}",
      "!src/setupTests.js",
      "!src/serviceWorker.js",
      "!src/index.js",
      "!src/util/sessionUtil.js",
      "!src/util/PropTypes.js",
      "!**/*.stories.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "coverageReporters": ["text"]
  },
  "description": "AWS Rekognition"
}
